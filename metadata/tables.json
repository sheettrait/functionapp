{
  "Patient": {
    "description": "Patient master record (static demographics).",
    "columns": [
      {"name": "PatientID", "semantic": "patient identifier"},
      {"name": "Sex", "semantic": "biological sex"},
      {"name": "DateOfBirth", "semantic": "date of birth"},
      {"name": "Age", "semantic": "age in years"},
      {"name": "ChronicConditions", "semantic": "chronic disease list"},
      {"name": "RiskFlags", "semantic": "high-risk flags"},
      {"name": "City", "semantic": "city of residence"},
      {"name": "NationalID_Fake", "semantic": "mock national ID"}
    ],
    "typical_questions": [
      "Who is the patient?",
      "What is the patient's age and sex?",
      "What chronic conditions or risk flags does the patient have?"
    ]
  },
